{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        
        "vmName": {
            "value": "YOUR-VM-NAME"
        },
        "adminUsername": {
            "value": "YOUR-VM-ADMIN-USERNAME"
        },
        "authenticationType": {
            "value": "password"
        },
        "adminPasswordOrKey": {
            "value": "YOUR-VM-PASSWORD-OR-KEY"
        },
        "dnsLabelPrefix": {
            "value": "YOUR-VM-DNS-NAME"
        },
        "vmSize": {
            "value": "Standard_B2s"
        },
        "virtualNetworkName": {
            "value": "YOUR-VNET-NAME"
        },
        "subnetName": {
            "value": "YOUR-VNET-subnet"
        },
        "networkSecurityGroupName": {
            "value": "YOUR-NSG-NAME"
        }
    }
}